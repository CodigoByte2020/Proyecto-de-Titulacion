<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="credito_seq" model="ir.sequence">
        <field name="name">Secuencia de Créditos</field>
        <field name="code">ventas.credito.cliente</field>
        <field name="prefix">CREDITO-</field>
        <field name="padding">8</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="pago_credito_seq" model="ir.sequence">
        <field name="name">Secuencia de Pagos de crédito</field>
        <field name="code">ventas.pago.credito.cliente</field>
        <field name="prefix">PAGO-CRED-</field>
        <field name="padding">8</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="venta_seq" model="ir.sequence">
        <field name="name">Secuencia de Ventas</field>
        <field name="code">ventas.ventas</field>
        <field name="prefix">VENTA-</field>
        <field name="padding">8</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>