<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ventas_seq" model="ir.sequence">
        <field name="name">Secuencia de Ventas</field>
        <field name="code">ventas</field>
        <field name="prefix">VENT-</field>
        <field name="padding">8</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="pago_seq" model="ir.sequence">
        <field name="name">Secuencia de Pagos</field>
        <field name="code">pago.credito.cliente</field>
        <field name="prefix">PAGO-</field>
        <field name="padding">8</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>